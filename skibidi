local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/HLong0000/th-nh/refs/heads/main/skibidi')))()

local Window = OrionLib:MakeWindow({Name = "ThịnhVip", HidePremium = false, SaveConfig = true, ConfigFolder = "dxl_bf"})

local Tab = Window:MakeTab({
	Name = "ScriptFarm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Mod Client",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

